<div class="photo">
	<a rel="photos" class="thumbnail lightbox" href="#{Strings::to_html attributes["src"].large}" title="#{attributes["alt"]}">
		<img src="#{Strings::to_html attributes["src"].photo}" alt="#{attributes["alt"]}" />
	</a>
	<div class="caption">#{Strings::to_html attributes["alt"]}</div>
</div>
