<div class="photo">
	<a rel="photos" class="thumbnail lightbox" href="#{Strings::to_html attributes["src"].large.to_s}" title="#{attributes["alt"]}">
		<img src="#{Strings::to_html attributes["src"].photo.to_s}" alt="#{attributes["alt"]}" />
	</a>
	<div class="caption">#{Strings::to_html attributes["alt"].to_s}</div>
</div>
