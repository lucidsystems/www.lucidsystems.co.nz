<?r subject = attributes['subject'] 
if subject ?>
<a class="link" href="/company/contact-us/index?subject=#{subject.to_html}" onclick="javascript:$('#contact [name=subject]').val('#{subject.to_html}');resetContactPopup(true); return false;">#{content == "" ? "contact us" : content}</a>
<?r else ?>
<a class="link" href="/company/contact-us/index" onclick="javascript:resetContactPopup(true); return false;">#{content == "" ? "contact us" : content}</a>
<?r end ?>